<Window x:Class="DataStructorSamples.TreeSamples"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataStructorSamples"
        mc:Ignorable="d"
        Title="TreeSamples" Height="450" Width="300">
    <Grid>
        <StackPanel>
            <Button x:Name="TestBinarySerachTreeBtn"
                    Content="二叉搜索树"
                    Click="TestBinarySerachTreeBtn_OnClick">
            </Button>

            <Button x:Name="TestAVLTreeBtn"
                    Content="AVL"
                    Click="TestAVLTreeBtn_OnClick">
            </Button>
        </StackPanel>
    </Grid>
</Window>
